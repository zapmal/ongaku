    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/platform-express": "^8.2.4",
    "@prisma/client": "^3.7.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dayjs": "^1.10.7",
    "dotenv-cli": "^4.1.1",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.2",
    "object-hash": "^3.0.0",
